    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+- m m",
    "command": "bookmarks.toggle",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+- m l",
    "command": "bookmarks.list",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+- c",
    "command": "workbench.action.tasks.runTask",
    "args": "Compile"
  },
  {
    "key": "ctrl+- b",
    "command": "workbench.action.tasks.runTask",
    "args": "build",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+- t",
    "command": "workbench.action.tasks.runTask",
    "args": "test",
